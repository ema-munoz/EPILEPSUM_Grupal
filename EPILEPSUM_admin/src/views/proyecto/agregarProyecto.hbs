<!-- Required meta tags -->
<link rel="stylesheet" href="/CSS/proyecto/agregarProyecto.css">

<div id="fondo">
    <img src="/img/fondos/Fondo.jpg" alt="Logo">
</div> 

<div id="contenedor">
    <!-- AquÃ­ comienza el primer formulario -->
    <div id="formulario">
        <h1 id="h1" name="h1">Agregar Proyecto</h1>
        <form action="/proyecto/agregar/" method="post">
            <div id="contenido">
                <div id="id">
                    {{#each proyecto}}
                    <input type="text" name="numero" id="numero" value="{{idProyecto}}">
                    {{/each}}
                </div>
                <div id="Nombre">
                    <label>Nombre Proyecto:</label>
                    <input type="text" name="nombreProyecto" id="nombre">
                </div>
                <div id="objetivos">
                    <label>Objetivos</label>
                    <input type="text" id="numeros" checked>
                    <button type="button" onclick="objetivos.crear()">Crear Objetivos</button>
                </div>
                <div id="listaObjetivos">

                </div>
                <div id="boton">
                    <button type="submit">Guardar</button>
                </div>
            </div>
        </form>
    </div>
</div>

<script src="/js/numero.js"></script>
<script src="/js/objetivos.js"></script>