<!-- Required meta tags -->
<link rel="stylesheet" href="/CSS/proyecto/listaProyecto.css">

<div id="fondo">
    <img src="/img/fondos/sistemaNervioso.png" alt="Logo">
</div>

<div id="contenedor">
    <div id="formulario">
        <form action="/proyecto/lista/{{user.idUsuario}}" method="get">
            <div id="contenido">
                {{#each proyecto}}
                <div id="nombre">
                    <label>{{nombreProyecto}}</label>
                </div>
                {{/each}}
                {{#each objetivos}}
                <div id="objetivos">
                    <ul>
                        <li>{{objetivoDetalleProyecto}}</li>
                        <button id="editar">
                            <a href="/proyecto/editar/{{idProyecto}}">Editar</a>
                        </button>
                        <button id="eliminar">
                            <a href="/proyecto/eliminar/{{idProyecto}}">Eliminar</a>
                        </button>
                        
                    </ul>
                </div>
                {{/each}}
            </div>
        </form>
    </div>
</div>
<script src="/js/numero.js"></script>
<script src="/js/objetivos.js"></script>